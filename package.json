{
  "name": "ui-scrollbar",
  "version": "0.0.1",
  "main": "index.js",
  "unpkg": "index.js",
  "module": "src/main.js",
  "less": "src/style.less",
  "stylus": "src/style.styl",
  "scss": "src/style.scss",
  "style": "style.css",
  "types": "index.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "demo:dev": "rollup -c rollup.website.js -w",
    "eslint": "eslint src --ext .js --fix",
    "format": "prettier --write src & npm run eslint",
    "start": "sirv docs",
    "prepare": "npm run build"
  },
  "author": "hezulong1 <https://github.com/hezulong1>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-json": "^4.0.2",
    "autoprefixer": "^9.7.6",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rollup": "^2.6.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-uglify": "^6.0.4",
    "sirv-cli": "^1.0.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "scroll-ease-efficient": "^1.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,docs/js}/**/*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
